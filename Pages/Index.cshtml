@page
@model Eduhunt.Pages.IndexModel

@{
    ViewData["Title"] = "Index";
}
<style>
    .carousel-inner img {
        width: 100%;
        height: 60%;
    }
</style>
    <!--start slide section-->
    <div class="d-flex justify-content-center my-5" style=" height: 350px; width:100%">
        <div id="carouselExampleAutoplaying" class="carousel slide bg-dark rounded-5" data-bs-ride="carousel"
             style="width:90%; height:100%;">
            <div class="carousel-inner justify-content-center align-items-center h-100 rounded-5">
                <div class="carousel-item active">
                    <div class="d-flex justify-content-center align-items-center h-100">
                        <img style="object-fit: cover; background-position: center;" class=" w-100 h-100"
                             src="https://static.unimates.edu.vn/wp-content/uploads/2021/07/Royal-Holloway-1.jpg"
                             alt="...">
                    </div>
                </div>
                <div class="carousel-item">
                    <div class="d-flex justify-content-center align-items-center h-100">
                        <img class=" w-100 h-100;background-position: center;"
                             src="https://static.unimates.edu.vn/wp-content/uploads/2021/07/Royal-Holloway-1.jpg"
                             alt="...">
                    </div>
                </div>
                <div class="carousel-item">
                    <div class="d-flex justify-content-center align-items-center h-100">
                        <img class=" w-100 h-100;background-position: center;"
                             src="https://static.unimates.edu.vn/wp-content/uploads/2021/07/Royal-Holloway-1.jpg"
                             alt="...">
                    </div>
                </div>
            </div>
            <button class="carousel-control-prev" type="button" style="left: -7rem"
                    data-bs-target="#carouselExampleAutoplaying" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" style="right: -7rem" type="button"
                    data-bs-target="#carouselExampleAutoplaying" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
            </button>
            <!--start text-->
            <div class="position-absolute text-start top-50 translate-middle"
                 style="color: white; margin-left: 31rem; font-size :4.5rem">
                <div class=" fw-bold">Scholarships:</div>
                <div class="">Your key to knowledge!</div>
            </div>
            <!--end text-->
        </div>
        <!--end slide section-->
    </div>
    <!--search bar-->
    <div class="container mt-5 shadow p-3 mb-5 bg-body-tertiary rounded">
        <form>
            <div class="row g-3">
                <!-- University Name Input -->
                <div class="col-md-2">
                    <label for="universityName" class="form-label">University Name</label>
                    <input type="text" class="form-control" id="universityName" placeholder="Enter university name">
                </div>
                <!-- Country Select Box -->
                <div class="col-md-2">
                    <label for="country" class="form-label">Country</label>
                    <select class="form-select" id="country">
                        <option selected disabled>Choose a country</option>
                        <option value="us">United States</option>
                        <option value="uk">United Kingdom</option>
                        <option value="canada">Canada</option>
                        <option value="australia">Australia</option>
                    </select>
                </div>
                <!-- City Select Box -->
                <div class="col-md-2">
                    <label for="city" class="form-label">City</label>
                    <select class="form-select" id="city">
                        <option selected disabled>Choose a city</option>
                        <option value="newyork">New York</option>
                        <option value="london">London</option>
                        <option value="toronto">Toronto</option>
                        <option value="sydney">Sydney</option>
                    </select>
                </div>
                <!-- Major Select Box -->
                <div class="col-md-2">
                    <label for="major" class="form-label">Major</label>
                    <select class="form-select" id="major">
                        <option selected disabled>Choose a major</option>
                        <option value="cs">Computer Science</option>
                        <option value="business">Business</option>
                        <option value="engineering">Engineering</option>
                        <option value="medicine">Medicine</option>
                    </select>
                </div>
                <!-- Level Select Box -->
                <div class="col-md-1">
                    <label for="level" class="form-label">Level</label>
                    <select class="form-select" id="level">
                        <option selected disabled>1</option>
                        <option value="undergraduate">Undergraduate</option>
                        <option value="graduate">Graduate</option>
                        <option value="phd">PhD</option>
                    </select>
                </div>
                <!-- Budget Input -->
                <div class="col-md-2">
                    <label for="budget" class="form-label">Budget</label>
                    <input type="number" class="form-control" id="budget" placeholder="Enter your budget">
                </div>
                <div class="col-md-1 d-flex align-items-end">
                    <button type="submit" class="btn" style="background-color: #00277F;">
                        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1"
